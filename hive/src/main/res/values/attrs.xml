<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HiveRecyclerView">
        <attr name="hive_horizontal_spacing" format="dimension" />
        <attr name="hive_vertical_spacing" format="dimension" />
        <attr name="items_count_in_row" format="integer" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>